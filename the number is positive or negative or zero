#include<stdio.h>
#include<conio.h>
int main()
{
  int a;
  clrscr();
  printf("\n Enter the number");
  scanf("%d",&a);
  if(a>0)
  {
    printf("\n The given number %d is Possitive",a);
  }
  else if(a<0)
  {
    printf("\n The given number %d is negative",a);
   }
  else if(a==0)
  {
    printf("\n The given number %d is Zero",a);
  }
  getch();
}
