#include<stdio.h>
#include<conio.h>
int main()
{
  int a;
  clrscr();
  printf("\n Enter the number");
  scanf("%d",&a);
  if(a>0)
  {
    printf("\n The given number is Possitive");
  }
  elseif(a<0)
  {
    printf("\n The given number is negative");
   }
  elseif(a==0)
  {
    printf("\n The given number is Zero");
  }
  getch();
}
